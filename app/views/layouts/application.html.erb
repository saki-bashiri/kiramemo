<!DOCTYPE html>
<html>
<head>
  <title>Kiramemo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="page-header">
      <h1><%= link_to "キラメモ", controller: :stamps, action: :index %></h1>
      <button class="btn header-upload"><%= link_to "Upload", controller: :stamps, action: :new %></button>
    </div>
    <%= yield %>
  </div>
</body>
</html>
